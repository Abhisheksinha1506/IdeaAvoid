{
  "name": "satuideas",
  "version": "1.0.0",
  "description": "A curated list of leads and pain points",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "enrich": "node server/scripts/enrichIdeas.js",
    "validate": "node server/scripts/validateAllIdeas.js",
    "validate:sample": "node server/scripts/validateAllIdeas.js --sample",
    "validate:internal": "node server/scripts/validateAllIdeas.js --internal-only",
    "validate:skip": "node server/scripts/validateAllIdeas.js --skip-existing",
    "validate:all": "node server/scripts/validateAllIdeas.js --internal-only --skip-existing",
    "validate:progress": "node server/scripts/checkProgress.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "natural": "^6.12.0",
    "node-cache": "^5.1.2",
    "openai": "^6.8.1",
    "puppeteer": "^24.29.1",
    "string-similarity": "^4.0.4",
    "vue": "^3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "concurrently": "^8.2.2",
    "vite": "^5.0.0"
  }
}
